    "bootstrap": "^5.3.3",
    "compression": "^1.8.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.20.0",
    "sirv": "^3.0.1"
  },
